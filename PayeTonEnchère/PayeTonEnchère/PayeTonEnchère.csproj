    </EmbeddedResource>
    <EmbeddedResource Update="Vues\EnchereVue.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Vues\InsciptionVue.xaml">
